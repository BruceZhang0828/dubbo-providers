<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="dubbo-provider" owner="dubbo-provider"/>
    <dubbo:registry protocol="zookeeper" address="192.168.10.41:4183" client="zkclient"/>
    <dubbo:protocol name="dubbo" port="-1"/>
    <dubbo:service interface="com.sharearn.dubbo.remote.TestService" ref="testService" timeout="10000"/>
    <bean id="testService" class="com.sharearn.dubbo.provider.impl.TestServiceImpl"/>
</beans>
